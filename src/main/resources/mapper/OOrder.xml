<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.peiwan.dao.OOrderMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.peiwan.bean.OOrder">
        <id column="oid" property="oid" />
        <result column="pid" property="pid" />
        <result column="person_name" property="personName" />
        <result column="o_service" property="oService" />
        <result column="o_xiadanshijian" property="oXiadanshijian" />
        <result column="o_content" property="oContent" />
        <result column="o_money" property="oMoney" />
        <result column="o_createtime" property="oCreatetime" />
        <result column="o_flate" property="oFlate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        pid, oid, person_name, o_service, o_xiadanshijian, o_content, o_money, o_createtime, o_flate
    </sql>

</mapper>
