<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.peiwan.dao.GServiceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.peiwan.bean.GService">
        <id column="pid" property="pid" />
        <result column="gid" property="gid" />
        <result column="g_name" property="gName" />
        <result column="g_daqu" property="gDaqu" />
        <result column="g_duanwei" property="gDuanwei" />
        <result column="p_gid" property="pGid" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        pid, gid, g_name, g_daqu, g_duanwei, p_gid
    </sql>

</mapper>
